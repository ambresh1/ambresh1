      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "sma1d_slow = SMA(infy, infy['Close'], 252) #12*21 of 1hr candels\n",
        "sma1d_fast = SMA(infy, infy['Close'], 600) #12*50 of 1hr candels\n",
        "sma_slow = SMA(infy, infy['Close'], 50)    \n",
        "sma_fast = SMA(infy, infy['Close'], 200)"
